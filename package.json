{
  "name": "luciole-reactjs",
  "version": "0.3.0",
  "description": "The reactjs frontend for luciole game",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "docs": "esdoc -c esdoc.json",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start-message": "babel-node tools/startMessage.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint:standard -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint:standard && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/app/**/*.js\" --reporter progress -r jsdom-global/register",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/app/**/*.js\" --reporter progress -r jsdom-global/register",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/app/**/*.js\" -r jsdom-global/register && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html",
    "lint:standard": "standard ./src/**/*.js --verbose",
    "help": "node tools/help.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js dev",
    "start:local": "npm-run-all --parallel test:watch open:src:local lint:watch",
    "open:src:local": "babel-node tools/srcServer.js localMode"
  },
  "author": "SebDez",
  "license": "MIT",
  "dependencies": {
    "chai-spies": "^0.7.1",
    "esdoc": "0.4.7",
    "object-assign": "4.1.0",
    "react": "15.3.2",
    "react-addons-pure-render-mixin": "15.2.1",
    "react-async-script": "0.8.0",
    "react-bootstrap": "0.30.6",
    "react-dom": "15.2.1",
    "react-fontawesome": "^1.2.0",
    "react-google-recaptcha": "0.8.0",
    "react-redux": "4.4.5",
    "react-redux-i18n": "0.0.3",
    "react-redux-modal": "0.5.2",
    "react-redux-toastr": "4.4.6",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "react-sidebar": "2.2.1",
    "react-svg": "1.1.5",
    "react-tooltip": "3.2.2",
    "redux": "3.5.2",
    "redux-form": "6.5.0",
    "redux-thunk": "2.1.0",
    "shrinkwrap": "0.4.0",
    "superagent": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.7",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.11.5",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "chai-datetime": "1.4.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.2.0",
    "coveralls": "2.11.11",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.1.1",
    "eslint-plugin-import": "1.11.1",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "https://registry.npmjs.org/jsdom/-/jsdom-7.2.2.tgz",
    "jsdom-global": "2.1.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "open": "0.0.5",
    "postcss-loader": "0.9.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.2.1",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "sinon-as-promised": "4.0.2",
    "sinon-chai": "2.8.0",
    "standard": "^7.1.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "standard": {
    "globals": [
      "before",
      "describe",
      "beforeEach",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "webpack-public-path.js"
    ]
  }
}
